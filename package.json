{
  "name": "lsh_web_App",
  "version": "1.0.0",
  "description": "Question Grid for LSH User Profiles and Job Postings",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel  open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* react_src tools",
    "lint-test": "eslint  react_src//components/Login/LoginPage.js ",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"react_src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist  lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "node tools/prodServer.js"
  },
  "author": "Bob De Vivo",
  "license": "",
